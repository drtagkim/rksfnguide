{
	"name":"FN Guide Access Helper",
	"description":"Taekyung Kim, FnGuide Helper 2019",
	"version":"0.0.5",
	"manifest_version":2,
	"browser_action":{
		"default_icon":"fnguide.png",
		"default_title":"FnGuide Download"
	},
	"background":{
		"scripts":["background.js"],
		"persistent":false
	},
	"permissions":[
		"storage",
		"activeTab",
		"downloads",
		"http://*/*",
		"https://*/*",
		"tabs",
		"notifications"
	],
	"options_page":"options.html",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}